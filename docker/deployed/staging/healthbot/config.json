{
    "Metrics": {
        "Port": "9090"
    },
    "Log": {
        "Human": false,
        "Debug": true
    },
    "Target": "https://staging.tableland.network/rpc",
    "Chains": [
        {
            "Name": "optimism-kovan",
            "Probe": {
                "CheckInterval": "15s",
                "ReceiptTimeout": "20s",
                "SIWE": "${HEALTHBOT_OPTIMISM_KOVAN_SIWE}",
                "Tablename": "${HEALTHBOT_OPTIMISM_KOVAN_TABLE}"
            }
        },
        {
            "Name": "optimism-goerli",
            "Probe": {
                "CheckInterval": "15s",
                "ReceiptTimeout": "20s",
                "SIWE": "${HEALTHBOT_OPTIMISM_GOERLI_SIWE}",
                "Tablename": "${HEALTHBOT_OPTIMISM_GOERLI_TABLE}"
            }
        }
    ]
}